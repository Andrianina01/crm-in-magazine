        //   //           //validation email 
        var valideEmail = 1
        var email = $('#softivendor_crmbundle_business_userBusiness_' + id + '_email');
        var errorMail = $('.error-email-' + id);
        var regexMail = /^[\w\-\+]+(\.[\w\-]+)*@[\w\-]+(\.[\w\-]+)*\.[\w\-]{2,19}$/;
