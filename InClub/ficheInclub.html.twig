<script>
    $(document).on('click', '.hideme', function() {

        var id = $(this).parent().data('id');


        //fiileAvatar
        var validefileAvatar = 1;
        var fileAvatar = $('#softivendor_crmbundle_business_userBusiness_' + id + '_fileAvatar ');
        var errorfileAvatar = $('.error-fileAvatar-' + id);
        var fileName = fileAvatar.val(),
            idxDot = fileName.lastIndexOf(".") + 1,
            extFile = fileName.substr(idxDot, fileName.length).toLowerCase();
        if (extFile == "jpg" || extFile == "jpeg" || extFile == "png" || fileName == "") {
            // fileAvatar.value = "";
        } else {
